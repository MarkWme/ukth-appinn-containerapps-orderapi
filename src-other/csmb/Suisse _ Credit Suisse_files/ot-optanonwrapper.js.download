	// OT Consent does not exists

	if (document.cookie.indexOf('OptanonAlertBoxClosed') < 0){
			
		// Block scroll page
		
		jQuery('body').addClass('ot-consent-manager-open');
			
		// If consent does not exists, remove PWP modals	
		
		if(ot_platform == 'pw'){

			// If country selector exists, accept it

			if (jQuery('.m-country-switch').length > 0) {

				jQuery('.m-country-switch').hide();
				jQuery('body').removeClass('no-scrolling');
				
				jQuery('.a-primary-button--country-switch').click();
			}

			if (jQuery('.country_check_notification_modal').length > 0) {

				if (location.pathname.search(/^\/global\//) !== -1) {
					jQuery('.country_check_notification_modal').hide();
				} else {
					jQuery('.country_check_notification_modal').remove();
				}
				
				var html_element = document.querySelector('html');
				jQuery(html_element).removeAttr('style');
			}
		}	
	}
	
	// Remove cookies created by Adobe Campaign if consent is not given

	if (document.cookie.indexOf('OptanonAlertBoxClosed') < 0 || (document.cookie.indexOf('OptanonAlertBoxClosed') > -1 && document.cookie.indexOf('groups=C0001%3A1%2CC0002%3A0') > -1)) {

		if (document.cookie.indexOf('AMCV_14CE34B8527836E60A490D44%40AdobeOrg') > -1) {

