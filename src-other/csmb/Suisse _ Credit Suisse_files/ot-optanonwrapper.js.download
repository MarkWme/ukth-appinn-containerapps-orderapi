function OTWaitSwitchToChange(switch_id) {
		
	var ot_new_switch_status = (jQuery('#ot-group-id-'+switch_id).attr('aria-checked') != 'false') ? true : false;
				
    if(ot_switch_status[switch_id] == ot_new_switch_status) {
		
		setTimeout(function() {
			OTWaitSwitchToChange(switch_id);
		}, 500);
	   
    } else {
		
		var save_preference_btn_handler_status = 'enabled';
		
		ot_switch_C0002_status = (typeof jQuery('#ot-group-id-C0002').attr('aria-checked') !== typeof undefined && jQuery('#ot-group-id-C0002').attr('aria-checked') != 'false') ? true : false;
		ot_switch_C0003_status = (typeof jQuery('#ot-group-id-C0003').attr('aria-checked') !== typeof undefined && jQuery('#ot-group-id-C0003').attr('aria-checked') != 'false') ? true : false;
		ot_switch_C0004_status = (typeof jQuery('#ot-group-id-C0004').attr('aria-checked') !== typeof undefined && jQuery('#ot-group-id-C0004').attr('aria-checked') != 'false') ? true : false;
		
		ot_switch_status = {
			'C0002': ot_switch_C0002_status,
			'C0003': ot_switch_C0003_status,
			'C0004': ot_switch_C0004_status
		};		
		
		if(ot_switch_status['C0003'] && !ot_switch_status['C0002']){ 

			save_preference_btn_handler_status = 'disabled';
			jQuery('#ot-desc-id-C0003-note').show();						
		};
		
		if(ot_switch_status['C0004'] && (!ot_switch_status['C0003'] || !ot_switch_status['C0002'])){ 	
						
			save_preference_btn_handler_status = 'disabled';
			jQuery('#ot-desc-id-C0004-note').show();
		};	
		
		if(save_preference_btn_handler_status != 'disabled'){
			
			jQuery('#ot-desc-id-C0003-note, #ot-desc-id-C0004-note').hide();
			jQuery('#onetrust-pc-sdk .save-preference-btn-handler').removeAttr('disabled');
		}
		
		return;
    }	
}

function OTUpdateSwitchLabels(){
	
	if(jQuery('#onetrust-consent-sdk .ot-switch-custom-label').length > 0){
		
		jQuery('#onetrust-consent-sdk .ot-switch-custom-label').remove();
	}
		
	jQuery('#onetrust-consent-sdk .ot-switch.ot-toggle').each(function() {
		
		if(jQuery(this).find('.switch-checkbox').attr('aria-checked') == 'true'){
			var ot_switch_label = '<span class="ot-switch-custom-label ot-switch-on">'+ot_banner_translations[ot_lang]['yes']+'</span>';
		} else {
			var ot_switch_label = '<span class="ot-switch-custom-label ot-switch-off">'+ot_banner_translations[ot_lang]['no']+'</span>';
		}
		
		jQuery(this).append(ot_switch_label);
		jQuery(this).find('.ot-switch-custom-label').css('left', ot_banner_translations[ot_lang]['ot-switch-custom-label-left']);
	});
	
	return;
}

