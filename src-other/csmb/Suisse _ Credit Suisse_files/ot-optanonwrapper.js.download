var OneTrustDOMContentLoaded = function() {		

	jQuery(document).on('click', '#onetrust-consent-sdk .ot-switch-label', function (event) {
				
		if(jQuery(this).next('.ot-switch-custom-label').hasClass('ot-switch-on')){
			
			jQuery(this).next('.ot-switch-custom-label').removeClass('ot-switch-on').addClass('ot-switch-off');
			jQuery(this).next('.ot-switch-custom-label').text(ot_banner_translations[ot_lang]['no']);
			
		} else {
			
			jQuery(this).next('.ot-switch-custom-label').removeClass('ot-switch-off').addClass('ot-switch-on');
			jQuery(this).next('.ot-switch-custom-label').text(ot_banner_translations[ot_lang]['yes']);
		}
				
		jQuery('#ot-desc-id-C0003-note, #ot-desc-id-C0004-note').hide();
		
		// Disable button until check status
		
		jQuery('#onetrust-pc-sdk .save-preference-btn-handler').attr('disabled', 'disabled');
		
		OTWaitSwitchToChange(jQuery(this).attr('for').substr(-5));		
	});
	
	jQuery(document).keyup(function(event) {
			
		if(event.which  == '32') {
				
			if(jQuery('body').hasClass('ot-consent-manager-open')){
						
				var _this = jQuery(':focus').next('.ot-switch-label');
							
				if(_this.next('.ot-switch-custom-label').hasClass('ot-switch-on')){
					
					_this.next('.ot-switch-custom-label').removeClass('ot-switch-on').addClass('ot-switch-off');
					_this.next('.ot-switch-custom-label').text(ot_banner_translations[ot_lang]['no']);
					
				} else {
					
					_this.next('.ot-switch-custom-label').removeClass('ot-switch-off').addClass('ot-switch-on');
					_this.next('.ot-switch-custom-label').text(ot_banner_translations[ot_lang]['yes']);
				}
						
				jQuery('#ot-desc-id-C0003-note, #ot-desc-id-C0004-note').hide();
				
				// Disable button until check status
				
				jQuery('#onetrust-pc-sdk .save-preference-btn-handler').attr('disabled', 'disabled');
				
				OTWaitSwitchToChange(_this.attr('for').substr(-5));	
			}
		}
	});
	
	// Click on Configure Cookies footer link
	
	jQuery(document).on('click', '.cookie-consent-manager--open', function (event) {

		event.preventDefault();

		// Block scroll page
		
		jQuery('body').addClass('ot-consent-manager-open');

		OneTrust.ToggleInfoDisplay();
	});
	
	// Unblock scroll page
	
	jQuery(document).on('click', '#onetrust-consent-sdk #onetrust-accept-btn-handler, #onetrust-consent-sdk .save-preference-btn-handler, #onetrust-consent-sdk #accept-recommended-btn-handler, #onetrust-reject-all-handler', function (event) {
		
		jQuery('body').removeClass('ot-consent-manager-open');
	});
	
	jQuery(document).on('keydown', function(event) {
		
		if(event.key == 'Escape') {
			
			if(jQuery('body').hasClass('ot-consent-manager-open')){
				
				jQuery('body').removeClass('ot-consent-manager-open');
			}
		}
	});
	
	// Click on Accept all and Continue on Preference center
	
	jQuery(document).on('click', '#onetrust-pc-sdk .save-preference-btn-container #accept-recommended-btn-handler', function (event) {
		
		OneTrust.AllowAll();
	});
		
};

